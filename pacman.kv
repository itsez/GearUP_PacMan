<Pac>:
    size: 50, 50
    canvas:
        Color:
            rgb: 255,238,0
        Ellipse:
            pos: self.pos
            size: self.size
            angle_start: (root.start_angle)
            angle_end: (root.end_angle)
<PacGame>:
    pac: pac_id

    Pac:
        id: pac_id
        center: self.parent.center